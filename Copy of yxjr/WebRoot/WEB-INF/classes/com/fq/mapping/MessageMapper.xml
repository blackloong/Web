    <select id="getAllLoginPage" resultType="Map" parameterType="Page">
		  		 SELECT m .*,
					t.name as uname
					 FROM tb_message m 
				    left JOIN tb_user t on t.id=m.userid
 			 <where>
                   m.isdeleted=2 
			   <if test="formData.pushtype!=null and formData.pushtype!='' ">
			  
			      and   m.pushtype=#{formData.pushtype}
			  </if>
			  
			   <if test="formData.messagetype!=null and formData.messagetype!='' ">
			  
			      and   m.messagetype=#{formData.messagetype}
			  </if>
		 
			    <if test="formData.uid!=null and formData.uid!='' ">
			     and m.userid=#{formData.uid}
			  
			  </if>
			  <!-- 用户姓名查询 -->
			     <if test="formData.uname!=null and formData.uname!='' ">
			     and t.name=#{formData.uname}
			  
			  </if>
			  
			    <!-- 已读 未读 -->
			     <if test="formData.isviewed!=null and formData.isviewed!='' ">
			     and m.isviewed=#{formData.isviewed}
			  
			  </if>
			  
			  
			  
			 </where>
			   <!-- 升序 -->
			     <if test="formData.sort==1">
			      ORDER BY pushdate asc
			  
			  </if>
			  
			    <!-- 降序 -->
			     <if test="formData.sort==2">
			      ORDER BY pushdate DESC
			  </if>	 
  </select>
  
  	<select id="messageCount" parameterType="formData" resultType="Map">
  		  SELECT COUNT(id) as count from tb_message where userid=#{uid} and pushtype='短信' and isviewed=2
  </select>
